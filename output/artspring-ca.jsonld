    "https://schema.org/headline": [
      {
        "@language": "en",
        "@value": "Parking"
      }
    ],
    "https://schema.org/text": [
      {
        "@language": "en",
        "@value": "ArtSpring patrons can use two parking lots  one behind the building, from McPhillips Ave, and an upper parking lot on Seaview Ave. There is also on-street parking in the surrounding area.\nWe have two wheelchair accessible parking spots by the front entrance, and two in the lower parking lot.\nCyclists will find a bike rack at the front of the building.\n"
      }
    ]
  },
