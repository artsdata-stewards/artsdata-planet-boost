  {
    "@id": "https://artspring.ca/temporary/01df66ce-25ad-4a77-81fd-0c2a2c7a893e",
    "@type": [
      "https://schema.org/CreativeWork"
    ],
    "http://www.w3.org/ns/prov#wasDerivedFrom": [
