        "@value": "https://tuxedov1.blob.core.windows.net/tuxedo-theatredelaville/show/La%20grande%20mascarade_-OZ9yRV-eYSaPppyFrFP_.png"
      }
    ],
    "http://ogp.me/ns#image:url": [
      {
        "@language": "fr",
        "@value": "https://tuxedov1.blob.core.windows.net/tuxedo-theatredelaville/show/La%20grande%20mascarade_-OZ9yRV-eYSaPppyFrFP_.png"
      }
