    "http://ogp.me/ns#image:url": [
      {
        "@language": "fr",
        "@value": "https://tuxedov1.blob.core.windows.net/tuxedo-theatredelaville/show/Penta%C3%A8dre_-ONpPscgYdLNaGnJie3t_.png"
      }
