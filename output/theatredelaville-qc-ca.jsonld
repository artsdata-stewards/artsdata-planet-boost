    "http://ogp.me/ns#image:url": [
      {
        "@language": "fr",
        "@value": "https://tuxedov1.blob.core.windows.net/tuxedo-theatredelaville/show/Mariana%20Mazza_-OdOUjrj1NdQm8veogW-_.png"
      }
