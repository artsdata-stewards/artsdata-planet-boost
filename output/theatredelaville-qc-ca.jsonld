    "http://ogp.me/ns#image:url": [
      {
        "@language": "fr",
        "@value": "https://tuxedov1.blob.core.windows.net/tuxedo-theatredelaville/show/Le%20bain%20(scolaire)_-OOU1kFeyyiEZof4QkX6_.png"
