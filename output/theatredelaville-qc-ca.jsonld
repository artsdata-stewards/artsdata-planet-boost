    "http://ogp.me/ns#image:url": [
      {
        "@language": "fr",
        "@value": "https://tuxedov1.blob.core.windows.net/tuxedo-theatredelaville/show/Trio%20Marianne%20Trudel_-Oat_04aOfN-D_GlOMDc_.png"
      }
