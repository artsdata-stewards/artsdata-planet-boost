    "http://ogp.me/ns#image:url": [
      {
        "@language": "fr",
        "@value": "https://tuxedov1.blob.core.windows.net/tuxedo-theatredelaville/show/Ma%20m%C3%A8re%2C%20Dieu%20et%20Sylvie%20Vartan_-OXTweARhZxna7-T_yTu_.png"
