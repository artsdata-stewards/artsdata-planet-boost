    "http://schema.org/text": [
      {
        "@language": "en",
        "@value": "The band will play for two 45 min sets with a brief intermission."
      }
    ],
    "http://schema.org/inLanguage": [
      {
        "@language": "en",
        "@value": "en-US"
      }
